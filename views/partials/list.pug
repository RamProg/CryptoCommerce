h1 Products
    if data.length
        table.table
            thead
                tr
                    th(scope="col") #
                    th(scope="col") Title
                    th(scope="col") Price
                    th(scope="col") Thumbnail
            for e in data
                tr
                    th(scope="row")=e.id
                    th(scope="row")=e.title
                    th(scope="row")=e.price
                    th
                        img(src=e.thumbnail width="25" height="25")
    else
        h3 No hay productos

